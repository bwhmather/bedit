msgid "Drop Changes and _Reload"
msgstr "Одбаци измене и _поново учитај"

#: bedit/bedit-io-error-info-bar.c:1178 bedit/resources/gtk/menus-common.ui:148
#: bedit/resources/gtk/menus.ui:9 bedit/resources/gtk/menus-traditional.ui:9
